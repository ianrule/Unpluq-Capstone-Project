{
    "manifest_version": 3,
    "name": "Unpluq",
    "description": "Website Blocker",
    "version": "1.0",
    "action": {
      "default_popup": "home.html",
      "default_icon": "unpluq.jpg"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["unpluq.jpg"],
        "matches": ["<all_urls>"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs"
    ]
  }